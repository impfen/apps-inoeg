  "type": "module",
  "dependencies": {
    "dayjs": "^1.10.7",
    "vanellus": "workspace:^0.8.0"
  },
  "devDependencies": {
    "@iconify-json/carbon": "^1.1.1",
    "@impfen/browserslist-config": "workspace:^0.8.0",
    "@impfen/common": "workspace:^0.8.0",
    "@impfen/eslint-config": "workspace:^0.8.0",
    "@impfen/jest-config": "workspace:^0.8.0",
    "@impfen/postcss-config": "workspace:^0.8.0",
    "@impfen/stylelint-config": "workspace:^0.8.0",
    "@sveltejs/adapter-static": "next",
    "@sveltejs/kit": "next",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-compat": "^4.0.2",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-regexp": "^1.5.1",
    "eslint-plugin-svelte3": "^3.4.1",
    "eslint-plugin-testing-library": "^5.0.5",
    "jest": "^27.5.1",
    "mdsvex": "^0.10.5",
    "postcss": "^8.4.6",
    "postcss-import": "^14.0.2",
    "postcss-load-config": "^3.1.3",
    "postcss-preset-env": "^7.4.1",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.6.0",
    "svelte": "^3.46.4",
    "svelte-check": "^2.4.5",
    "svelte-intl-precompile": "^0.8.1",
    "svelte-preprocess": "^4.10.4",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5",
    "unplugin-icons": "^0.13.2"
  },
  "jest": {
    "preset": "@impfen/jest-config"
